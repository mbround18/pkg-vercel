{
  "name": "pkg-demo",
  "packageManager": "yarn@3.4.1",
  "devDependencies": {
    "pkg": "5.8.1",
    "typescript": "4.9.5",
    "vite": "4.1.4"
  },
  "bin": "./server/index.js",
  "scripts": {
    "build": "vite build ./client",
    "docker:build": "docker build . --build-arg \"JFROG_NPM_TOKEN=${JFROG_NPM_TOKEN}\" --tag fst/pkg-poc:dev",
    "docker:run": "docker run --rm -it --init -p 3000:3000 fst/pkg-poc:dev"
  },
  "pkg": {
    "scripts": "server/**/*.js",
    "assets": "client/dist/**/*",
    "outputPath": "dist"
  },
  "dependencies": {
    "express": "4.18.2"
  }
}
